{
  "name": "TestTool",
  "displayName": "Test Tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "xml",
        "path": "./snippets/TestToolSnippets.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "TestTool.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "Test Tool",
      "properties": {}
    }
  },
  "activationEvents": [
    "onCommand:TestTool.helloWorld"
  ]
}